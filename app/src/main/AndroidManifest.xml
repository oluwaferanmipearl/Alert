<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19alert">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_pop"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_pop"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".QuizStart"></activity>

        <activity
            android:name=".Spreads"
            android:label="Spread"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar" />

        <activity
            android:name=".TotalCases"
            android:label="Total Cases"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar" />

        <activity
            android:name=".prevention"
            android:label="Prevention"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar" />

        <activity
            android:name=".Symptoms"
            android:label="Symptoms"
            android:parentActivityName=".MainActivity"
            android:theme="@style/ActionBar" />

        <activity
            android:name=".SplashScreen"
            android:theme="@style/SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>